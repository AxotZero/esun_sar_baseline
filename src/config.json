{
    "name": "PetPoc",
    "n_gpu": 1,
    "arch": {
        "type": "SarModel",
        "args": {
            "num_cat_pkl_path": "/media/hd03/axot_data/sar/data/num_cat_dict.yml",
            "emb_feat_dim": 32,
            "hidden_size": 128,
            "hidden_size_coeff": 6,
            "dropout": 0.2,
            "temporal_aggregator_type": "TemporalGruAggregator",
            "temporal_aggregator_args": {
                "input_size": 128,
                "hidden_size": 384,
                "num_layers": 3,
                "dropout": 0.2
            }
        }
    },
    "data_loader": {
        "type": "MaxLenDataLoader",
        "args": {
            "data_path": "/media/hd03/axot_data/sar/data/cust_data.pkl",
            "max_len": 512,
            "batch_size": 128,
            "shuffle": true,
            "validation_split": 0.2,
            "num_workers": 4,
            "fold_idx": 0,
            "training": true
        }
    },
    "optimizer": {
        "type": "Ranger",
        "args": {
            "lr": 0.001,
            "weight_decay": 1e-05
        }
    },
    "loss": "weighted_bce_loss",
    "metrics": [
        "recall_n"
    ],
    "trainer": {
        "epochs": 100,
        "save_dir": "../save_dir/test",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 4,
        "training": true,
        "tensorboard": false
    }
}